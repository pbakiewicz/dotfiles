alias e='nvim'
alias vim="nvim"
alias reload=". $ZDOTDIR/.zshrc"
alias ls="ls --color"
alias ..="cd .."
alias ...="cd ../.."
# muszę tutaj jeszcze nad tym popracować. W bashu mam piękny fzf.
# a tutaj musza walic te ctrl+r aby zobaczyć kolejny wynik. bo widzę
# tylko jeden at the time.
bindkey '^R' history-incremental-search-backward
